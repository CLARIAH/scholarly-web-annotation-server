@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

beng:Movie a rdfs:Class ;
    rdfs:label "Movie" ;
    hi:hasMetadataItem beng:Date,
        beng:Description,
        beng:Identifier,
        beng:InCollection,
        beng:Title ;
    hi:hasRepresentation hi:Representation ;
    rdfs:subClassOf hi:CreativeWork .

beng:Program a rdfs:Class ;
    rdfs:label "BenG_Program" ;
    hi:hasMetadataItem beng:Broadcaster,
        beng:BroadcasterDate,
        beng:Date,
        beng:Description,
        beng:Genre,
        beng:Identifier,
        beng:InCollection,
        beng:Summary,
        beng:Title ;
    hi:hasRepresentation hi:Representation ;
    hi:isPartOf beng:Season ;
    rdfs:subClassOf hi:CreativeWork .

beng:Season a rdfs:Class ;
    rdfs:label "BenG_Season" ;
    hi:hasMetadataItem beng:Broadcaster,
        beng:Genre,
        beng:Summary,
        beng:Title ;
    hi:isPartOf beng:Series ;
    rdfs:subClassOf hi:CreativeWork .

beng:Segment a rdfs:Class ;
    rdfs:label "BenG_Segment" ;
    hi:hasMetadataItem beng:Summary,
        beng:Title ;
    hi:hasRepresentation hi:Representation ;
    hi:isPartOf beng:Program ;
    rdfs:subClassOf hi:CreativeWork .

beng:Series a rdfs:Class ;
    rdfs:label "BenG_Series" ;
    hi:hasMetadataItem beng:Broadcaster,
        beng:Genre,
        beng:Summary,
        beng:Title ;
    rdfs:subClassOf hi:CreativeWork .

beng:BroadcastDate a owl:Class ;
    rdfs:label "BroadcastDate" ;
    rdfs:subClassOf hi:MetadataItem .

beng:hasItem a owl:ObjectProperty ;
    rdfs:label "hasItem" ;
    rdfs:comment "Indicates that a collection contains a creative work" ;
    rdfs:domain beng:Collection ;
    rdfs:range hi:CreativeWork ;
    rdfs:subPropertyOf hi:includes .

<http://localhost:3000/vocabularies/delpher_ontology.ttl> owl:imports <http://localhost:3000/vocabularies/genericontology.ttl> .

dlp:ArticleType a owl:Class ;
    rdfs:label "ArticleType" ;
    rdfs:subClassOf dlp:MetadataItem .

dlp:Date a owl:Class ;
    rdfs:label "Date" ;
    rdfs:subClassOf dlp:MetadataItem .

dlp:DateNote a owl:Class ;
    rdfs:label "DateNote" ;
    rdfs:subClassOf dlp:Note .

dlp:LocationNote a owl:Class ;
    rdfs:label "LocationNote" ;
    rdfs:subClassOf dlp:Note .

dlp:Newspaper a owl:Class ;
    rdfs:label "Newspaper" ;
    rdfs:subClassOf hi1:CreativeWork .

dlp:NewspaperArticle a owl:Class ;
    rdfs:label "NewspaperArticle" ;
    rdfs:subClassOf hi1:CreativeWork .

dlp:Number a owl:Class ;
    rdfs:label "Number" ;
    rdfs:subClassOf dlp:MetadataItem .

dlp:ParagraphInArticle a owl:Class ;
    rdfs:label "ParagraphInArticle" ;
    rdfs:subClassOf hi1:CreativeWork .

dlp:SourceNote a owl:Class ;
    rdfs:label "SourceNote" ;
    rdfs:subClassOf dlp:Note .

dlp:hasMetadataItem a owl:ObjectProperty ;
    rdfs:label "hasMetadataItem" ;
    rdfs:comment "Adds any type of MetadataItem to creative object" ;
    rdfs:domain hi1:CreativeWork ;
    rdfs:range dlp:MetadataItem ;
    rdfs:subPropertyOf hi1:hasEnrichment .

dlp:hasNote a owl:ObjectProperty ;
    rdfs:label "hasNote" ;
    rdfs:comment "Adds any type of note to creative object" ;
    rdfs:domain hi1:CreativeWork ;
    rdfs:range dlp:Note ;
    rdfs:subPropertyOf hi1:hasEnrichment .

<http://localhost:3000/vocabularies/dlp> a owl:Ontology .

<http://localhost:3001/vocabularies/beng> a owl:Ontology .

<http://localhost:3001/vocabularies/beng_ontology.ttl> owl:imports <http://localhost:3001/vocabularies/genericontology.ttl> .

<http://localhost:3001/vocabularies/eye_ontology.ttl> owl:imports <http://localhost:3001/vocabularies/genericontology.ttl> .

hi:Data a owl:Class ;
    rdfs:label "Data" ;
    rdfs:subClassOf hi:Representation .

hi:IgnorableElement a owl:Class ;
    rdfs:label "IgnorableElement" ;
    rdfs:comment "Element should be ignored in annotation" ;
    rdfs:subClassOf hi:AnnotatableThing .

hi:IgnorableTag a owl:Class ;
    rdfs:label "IgnorableContent" ;
    rdfs:comment "Tags of element should be ignored in annotation (content is relevant)" ;
    rdfs:subClassOf hi:AnnotatableThing .

hi:Image a owl:Class ;
    rdfs:label "Image" ;
    rdfs:subClassOf hi:Representation .

hi:MediaBearer rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom hi:AboutMediaBearer ;
            owl:onProperty hi:hasAnnotation ],
        [ a owl:Restriction ;
            owl:allValuesFrom hi:AboutMediaBearer ;
            owl:onProperty hi:hasAnnotation ],
        [ a owl:Restriction ;
            owl:allValuesFrom hi:AboutMediaBearer ;
            owl:onProperty hi:hasAnnotation ] .

hi:Text a owl:Class ;
    rdfs:label "Text" ;
    rdfs:subClassOf hi:Representation .

hi:Video a owl:Class ;
    rdfs:label "Video" ;
    rdfs:subClassOf hi:Representation .

hi:hasMetadataItem a owl:ObjectProperty ;
    rdfs:label "hasMetadataItem" ;
    rdfs:comment "Adds any type of MetadataItem to creative object" ;
    rdfs:domain hi:CreativeWork ;
    rdfs:range hi:MetadataItem ;
    rdfs:subPropertyOf hi:hasEnrichment .

hi:hasRepresentation a owl:ObjectProperty ;
    rdfs:label "hasRepresentation" ;
    rdfs:comment "Connects a Creative Work to its representation" ;
    rdfs:domain hi:CreativeWork ;
    rdfs:range hi:Representation ;
    rdfs:subPropertyOf hi:includes .

hi:hasType a owl:DatatypeProperty ;
    rdfs:label "hasType" ;
    rdfs:comment "Can be used to assign a more specific type to an AnnotatableThing" ;
    rdfs:domain hi:AnnotatableThing ;
    rdfs:range xsd:string .

hi:isCarriedOn a owl:ObjectProperty ;
    rdfs:label "isCarriedOn" ;
    rdfs:comment "Connects a Creative Work to its physical bearers (usually pages)" ;
    rdfs:domain hi:CreativeWork ;
    rdfs:range hi:TextBearer ;
    rdfs:subPropertyOf hi:includes .

hi:isPartOf a owl:ObjectProperty ;
    rdfs:label "isPartOf" ;
    rdfs:comment "Describes relation between parts of Works and Works" ;
    rdfs:domain hi:CreativeWork ;
    rdfs:range hi:CreativeWork ;
    owl:inverseOf hi:hasPart .

hi:isRepresentationOf a owl:ObjectProperty ;
    rdfs:label "isRepresentationOf" ;
    rdfs:comment "Connects a Representation to its Creative Work" ;
    rdfs:domain hi:Representation ;
    rdfs:range hi:CreativeWork ;
    rdfs:subPropertyOf hi:includes .

hi:selectWholeElement a owl:DatatypeProperty ;
    rdfs:label "hasType" ;
    rdfs:comment "Selecting part of element is not possible" ;
    rdfs:domain hi:AnnotatableThing ;
    rdfs:range xsd:boolean .

<http://localhost:3001/vocabularies/vangoghontology.ttl> owl:imports <http://localhost:3001/vocabularies/genericontology.ttl> .

