beng:Collection a owl:Class ;
    rdfs:label "Collection" .

hi:TextBearer a owl:Class ;
    rdfs:label "TextBearer" ;
    rdfs:subClassOf hi:AnnotatableThing .

hi:hasPart a owl:ObjectProperty ;
